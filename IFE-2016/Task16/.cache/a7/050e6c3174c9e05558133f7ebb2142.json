lumn":27},"source":"task.js","original":{"line":104,"column":31}},{"generated":{"line":101,"column":37},"source":"task.js","original":{"line":104,"column":8}},{"generated":{"line":103,"column":0},"source":"task.js","original":{"line":105,"column":8}},{"name":"e","generated":{"line":103,"column":8},"source":"task.js","original":{"line":105,"column":12}},{"generated":{"line":103,"column":9},"source":"task.js","original":{"line":105,"column":13}},{"name":"target","generated":{"line":103,"column":10},"source":"task.js","original":{"line":105,"column":14}},{"generated":{"line":103,"column":16},"source":"task.js","original":{"line":105,"column":12}},{"name":"nodeName","generated":{"line":103,"column":17},"source":"task.js","original":{"line":105,"column":21}},{"generated":{"line":103,"column":25},"source":"task.js","original":{"line":105,"column":12}},{"name":"toLowerCase","generated":{"line":103,"column":26},"source":"task.js","original":{"line":105,"column":30}},{"generated":{"line":103,"column":37},"source":"task.js","original":{"line":105,"column":12}},{"generated":{"line":103,"column":43},"source":"task.js","original":{"line":105,"column":47}},{"generated":{"line":103,"column":51},"source":"task.js","original":{"line":105,"column":8}},{"generated":{"line":103,"column":53},"source":"task.js","original":{"line":105,"column":57}},{"generated":{"line":104,"column":0},"source":"task.js","original":{"line":105,"column":58}},{"name":"delBtnHandle","generated":{"line":105,"column":0},"source":"task.js","original":{"line":106,"column":12}},{"name":"delBtnHandle","generated":{"line":105,"column":6},"source":"task.js","original":{"line":106,"column":12}},{"generated":{"line":105,"column":18},"source":"task.js","original":{"line":106,"column":24}},{"generated":{"line":106,"column":0},"source":"task.js","original":{"line":107,"column":9}},{"generated":{"line":107,"column":0},"source":"task.js","original":{"line":108,"column":5}},{"generated":{"line":107,"column":3},"source":"task.js","original":{"line":101,"column":4}},{"generated":{"line":107,"column":5},"source":"task.js","original":{"line":108,"column":7}},{"generated":{"line":107,"column":10},"source":"task.js","original":{"line":101,"column":4}},{"generated":{"line":108,"column":0},"source":"task.js","original":{"line":110,"column":1}},{"name":"init","generated":{"line":110,"column":0},"source":"task.js","original":{"line":112,"column":0}},{"generated":{"line":110,"column":4},"source":"task.js","original":{"line":112,"column":4}}],"sources":{"task.js":"// 1.正则匹配的方法 表单验证\r\n// 2.输入更新render，有没有更好的方法？\r\n// 3.分层处理的结构：设置listener==>handler==>具体操作函数\r\n// 4.事件代理\r\n// 5.多层函数怎么\r\n\r\n\r\nvar aqiData = {\r\n    // \"北京\": 90,\r\n    // \"上海\": 40\r\n}\r\n\r\n\r\nfunction addAqiData() {//因为是绑定点击提交按钮这个事件，所以在点击按钮时再获取表单内容，也就不用担心多层调用参数传递问题\r\n    cityInput = document.getElementById('aqi-city-input');\r\n    valueInput = document.getElementById('aqi-value-input');\r\n    // cityInput.addEventListener('keypress', checkCity, false);\r\n    // valueInput.addEventListener('keypress', checkValue, false);\r\n    reCity = /^[\\u4e00-\\u9fa5]+$/;//+表示一或者多个\r\n    reNum = /^-?\\d+$/;\r\n    city = cityInput.value;\r\n    if (!reCity.test(city)) {\r\n        alert('请输入中文城市名！')\r\n        return false//return false使得下面的对象也就不会被赋值了；当然还是阻止不了rendering的\r\n    }\r\n    value = valueInput.value;\r\n    if (!reNum.test(value)) {\r\n        alert('请输入数字！')\r\n        return false\r\n    }\r\n    aqiData[city] = value;\r\n    return true\r\n}\r\n\r\n// function checkCity(e) {\r\n//     if (e.charCode < 97 || e.charCode > 122) {//检查键盘按的是什么键的，没办法用来检查输入的是不是中文\r\n//         e.preventDefault();\r\n//         alert('请输入中文城市名！')\r\n//     }\r\n// }\r\n\r\n// function checkValue(e) {\r\n//     if (e.charCode < 97 || e.charCode > 122) {\r\n//         e.preventDefault();\r\n//         alert('请输入数字！')\r\n//     }\r\n// }\r\n\r\n\r\n\r\n// function delAqidata(event) {//多一层函数event就已经捕捉不到了，学怎么传递\r\n//     console.log(event);\r\n//     var city = event.target.parentNode.parentNode.childNodes[0].innerHTML;\r\n//     delete aqiData[city];\r\n// }\r\n\r\n\r\n/**\r\n * 渲染aqi-table表格\r\n */\r\nfunction renderAqiList(city, value) {\r\n    // console.log(\"I am rendering..\");\r\n    var table = document.getElementById('aqi-table')\r\n    html = '<tr><td> 城市</td><td>空气质量</td><td>操作</td></tr>'\r\n    Object.keys(aqiData).forEach(function (key) {\r\n        html += \"<tr><td>\" + key + \"</td><td>\" + aqiData[key] + \"</td><td><button>删除</button></td></tr>\"\r\n    })\r\n    table.innerHTML = html;\r\n}\r\n\r\n/**\r\n * 点击add-btn时的处理逻辑\r\n * 获取用户输入，更新数据，并进行页面呈现的更新\r\n */\r\nfunction addBtnHandle() {\r\n    toRender = addAqiData();\r\n    if (toRender) {//addAqiData检查是合法输入再来render\r\n        renderAqiList();\r\n    }\r\n}\r\n\r\n\r\n\r\n/**\r\n * 点击各个删除按钮的时候的处理逻辑\r\n * 获取哪个城市数据被删，删除数据，更新表格显示\r\n */\r\nfunction delBtnHandle(event) {\r\n    var city = event.target.parentNode.parentNode.childNodes[0].innerHTML;\r\n    delete aqiData[city];\r\n    renderAqiList();\r\n}\r\n\r\n//代码逻辑是集中在init中定义eventListener给两个handler\r\nfunction init() {\r\n\r\n    var addBtn = document.getElementById('add-btn');\r\n    addBtn.addEventListener('click', addBtnHandle);\r\n\r\n    var table = document.getElementById('aqi-table');\r\n    table.addEventListener('click', function (e) {\r\n        //下两行适配FF和IE，将event作为函数的参数传入例如e，然后为参数e添加适配方法\r\n        e = e || window.event;\r\n        target = e.target || e.srcElement;\r\n        if (e.target.nodeName.toLowerCase() == \"button\") {//必须要匹配到了是button 才会调用handler\r\n            delBtnHandle();\r\n        }\r\n    }, false)\r\n\r\n}\r\n\r\ninit();\r\n"},"lineCount":null}},"error":null,"hash":"bd40369f83dd42785147a741ddb9b16c","cacheData":{"env":{}}}